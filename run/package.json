{
  "name": "@cycle/run",
  "version": "6.0.0",
  "description": "The Cycle.js run() function",
  "license": "MIT",
  "homepage": "https://cycle.js.org",
  "bugs": "https://github.com/cyclejs/cyclejs/issues",
  "repository": "https://github.com/cyclejs/cyclejs/tree/master/run",
  "contributors": [
    {
      "name": "Andre Staltz",
      "email": "andre@staltz.com"
    },
    {
      "name": "Jan van Br√ºgge",
      "email": "supermanitu@gmail.com"
    }
  ],
  "keywords": [
    "reactive",
    "framework",
    "rxjs",
    "rx",
    "unidirectional",
    "mvi",
    "virtual-dom"
  ],
  "main": "build/bundle.cjs.js",
  "module": "build/bundle.esm.js",
  "types": "build/index.d.ts",
  "dependencies": {
    "@cycle/callbags": "^3.0.2"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/cyclejs"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json && pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "BUNDLE_FORMAT=cjs rollup -c ../configs/rollup.config.ts",
    "build:esm": "BUNDLE_FORMAT=esm rollup -c ../configs/rollup.config.ts",
    "test": "nyc --reporter text --reporter html pnpm run test:no_cover",
    "test:ci": "nyc --reporter text --reporter json pnpm run test:no_cover",
    "test:no_cover": "mocha --require ts-node/register test/*.ts",
    "format": "prettier --write './{src,test}/**/*'"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "../configs/.prettierrc.json",
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1"
  }
}
